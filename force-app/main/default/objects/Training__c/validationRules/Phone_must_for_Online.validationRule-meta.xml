<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_must_for_Online</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Is_Online__c , ISBLANK( Candidate_Phone__c ))</errorConditionFormula>
    <errorMessage>Phone Number is must if the class is Online</errorMessage>
</ValidationRule>
